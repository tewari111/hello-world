{
  "name": "vulnerable-app",
  "version": "1.0.0",
  "description": "A sample app with known vulnerabilities for testing Grype",
  "main": "index.js",
  "dependencies": {
    "express": "4.17.1",                // Vulnerabilities in Express 4.17.1
    "lodash": "4.17.19",                // Known prototype pollution vulnerability in lodash < 4.17.21
    "mongoose": "5.10.0",               // Various vulnerabilities in older Mongoose versions
    "debug": "2.6.8",                   // Prototype pollution in debug < 4.3.1
    "jquery": "3.4.1",                  // Cross-site scripting vulnerability in jQuery < 3.5.0
    "axios": "0.19.2",                  // Server-side request forgery in axios < 0.21.1
    "moment": "2.19.3",                 // Vulnerabilities in Moment < 2.19.4
    "marked": "0.3.6",                  // Cross-site scripting vulnerability in marked < 0.3.9
    "handlebars": "4.0.11"              // Prototype pollution vulnerability in handlebars < 4.5.3
  },
  "devDependencies": {
    "jest": "24.0.0",                   // Prototype pollution in older Jest versions
    "webpack": "4.0.0",                 // Prototype pollution in webpack < 4.46.0
    "grunt": "1.0.1"                    // Regular expression denial of service in grunt < 1.0.4
  },
  "scripts": {
    "start": "node index.js",
    "test": "jest"
  },
  "author": "Test Developer",
  "license": "MIT"
}
